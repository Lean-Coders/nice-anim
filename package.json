{
  "name": "nice-anim",
  "version": "0.1.1",
  "description": "An animate on scroll Web Component built with StencilJS",
  "keywords": [
    "animate",
    "scroll",
    "reveal"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/nice-anim/nice-anim.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nicell/nice-anim.git"
  },
  "bugs": {
    "url": "https://github.com/Nicell/nice-anim/issues"
  },
  "homepage": "https://github.com/Nicell/nice-anim",
  "license": "MIT"
}
